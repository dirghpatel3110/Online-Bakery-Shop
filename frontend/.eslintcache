ndler = (e) => {\n    e.preventDefault();\n    if (comment && rating) {\n      dispatch(\n        createReview(productId, { rating, comment, name: userInfo.name })\n      );\n    } else {\n      alert(\"Please enter comment and rating\");\n    }\n  };\n  return (\n    <div>\n      {loading ? (\n        <LoadingBox></LoadingBox>\n      ) : error ? (\n        <MessageBox variant=\"danger\">{error}</MessageBox>\n      ) : (\n        <div>\n          <Link to=\"/\" id=\"back\">\n            Back\n          </Link>\n\n          <div className=\"row top\">\n            <div className=\"col-2\">\n              <img\n                className=\"large\"\n                src={product.image}\n                alt={product.name}\n              ></img>\n            </div>\n\n            <div className=\"card card-body2\">\n              <div className=\"col-1\">\n                <ul>\n                  <li>\n                    <h1>{product.name}</h1>\n                  </li>\n                  <li>\n                    <Rating\n                      rating={product.rating}\n                      numReviews={product.numReviews}\n                    ></Rating>\n                  </li>\n\n                  <li className=\"desc\">{product.description}</li>\n                </ul>\n\n                <div className=\"card card-body\">\n                  <ul>\n                    <li>\n                      <div className=\"row\">\n                        <div>Price</div>\n                        <div className=\"price\">${product.price}</div>\n                      </div>\n                    </li>\n                    <li>\n                      <div className=\"row\">\n                        <div>Status</div>\n                        <div>\n                          {product.countInStock > 0 ? (\n                            <span className=\"success\">In Stock</span>\n                          ) : (\n                            <span className=\"danger\">Unavailable</span>\n                          )}\n                        </div>\n                      </div>\n                    </li>\n                    {product.countInStock > 0 && (\n                      <>\n                        <li>\n                          <div className=\"row\">\n                            <div>Qty</div>\n                            <div>\n                              <select\n                                value={qty}\n                                onChange={(e) => setQty(e.target.value)}\n                              >\n                                {[...Array(product.countInStock).keys()].map(\n                                  (x) => (\n                                    <option key={x + 1} value={x + 1}>\n                                      {x + 1}\n                                    </option>\n                                  )\n                                )}\n                              </select>\n                            </div>\n                          </div>\n                        </li>\n                        <li>\n                          <button\n                            onClick={addToCartHandler}\n                            className=\"primary block\"\n                          >\n                            Add to Cart\n                          </button>\n                        </li>\n                      </>\n                    )}\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n}\n","C:\\Users\\dirgh\\OneDrive\\Desktop\\ARC-Bakery-E-commerce-App-main\\frontend\\src\\components\\PrivateRoute.js",[],"C:\\Users\\dirgh\\OneDrive\\Desktop\\ARC-Bakery-E-commerce-App-main\\frontend\\src\\screens\\ShippingAddressScreen.js",[],"C:\\Users\\dirgh\\OneDrive\\Desktop\\ARC-Bakery-E-commerce-App-main\\frontend\\src\\screens\\SigninScreen.js",[],"C:\\Users\\dirgh\\OneDrive\\Desktop\\ARC-Bakery-E-commerce-App-main\\fronten